{
  "name": "tax-organizer",
  "version": "0.0.1",
  "description": "gather and organizer tax information",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf static/dist",
    "start": "npm run start-prod",
    "start-prod": "NODE_ENV=production node index.js",
    "start-dev": "node index.js",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "watch-client": "node webpack-dev-server",
    "dev": "concurrent --kill-others \"npm run start-dev\" \"npm run watch-client\"",
    "lint": "eslint -c .eslintrc src/",
    "test": "NODE_ENV=test mocha ./src/**/__tests__/*-test.js --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kikoku/tax-organizer.git"
  },
  "bugs": {
    "url": "https://github.com/kikoku/tax-organizer/issues"
  },
  "contributors": [
    "Collin Delphia <collin@fateseal.com>"
  ],
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "4.2.1",
    "npm": "3.3.9"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "axios": "^0.9.0",
    "babel": "^5.8.5",
    "babel-loader": "^5.3.2",
    "bcrypt": "^0.8.5",
    "bluebird": "2.9.34",
    "boom": "^3.1.1",
    "classnames": "^2.2.3",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "hapi": "^12.1.0",
    "hapi-auth-jwt2": "^5.3.2",
    "hapi-qs": "^1.0.0",
    "history": "^1.17.0",
    "inert": "^3.2.0",
    "joi": "^7.2.2",
    "jwt-simple": "^0.3.0",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "mongoose": "^4.3.6",
    "mongoose-timestamp": "^0.5.0",
    "node-sass": "^3.4.2",
    "qs": "^6.0.2",
    "react": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "react-document-meta": "^2.0.1",
